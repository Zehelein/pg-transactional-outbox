{
  "name": "consumer",
  "version": "1.0.0",
  "description": "Message receiver using the transactional inbox pattern.",
  "main": "./dist/index.js",
  "author": "Frank Zehelein",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "start": "node dist/index.js",
    "dev:watch": "tsc-watch --onSuccess \"node dist/index.js\"",
    "debug:watch": "tsc-watch --onSuccess \"node --inspect dist/index.js\"",
    "setup:db": "ts-node ./setup/init-db.ts",
    "export:db": "ts-node ./setup/export-db-scripts.ts"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "async-mutex": "^0.4.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "rascal": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.4",
    "@types/pg": "^8.10.9",
    "@types/rascal": "^10.0.9",
    "jest": "^29.7.0",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "5.3.2"
  }
}
